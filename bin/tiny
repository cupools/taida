#!/usr/bin/env node

var program = require('commander')
var PKG = require('../package.json')

program
  .version(PKG.version)
  .usage('[command] [options]')
  .allowUnknownOption()

program
  .command('compress', 'default task', {
    isDefault: true
  })
  .command('apikey', 'manage apikeys of https://tinypng.com')

program
  .parse(process.argv)

if (!process.argv.slice(2).length) {
  program.outputHelp()
}
