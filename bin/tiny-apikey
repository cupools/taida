#!/usr/bin/env node

var program = require('commander')

program
  .usage('[options]')
  .option('-a, --add <apikeys...>', 'add apikeys for usage')
  .option('-d, --del <apikeys...>', 'remove apikeys')
  .option('   --list', 'show apikeys list')
  .option('   --edit', 'edit apikeys in editor')
  .option('   --clear', 'remove all apikeys')
  .parse(process.argv)

require('../lib/cmd/apikey').default()
