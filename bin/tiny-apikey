#!/usr/bin/env node

var program = require('commander')
var PKG = require('../package.json')
var apikey = require('../lib/cmd/apikey')

program
  .version(PKG.version)
  .usage('<cmd> [keys]')

program
  .command('add <keys...>')
  .description('add apikeys for usage')
  .action(function (keys) {
    console.log(keys)
  })

program
  .command('delete <keys...>')
  .description('remove apikeys')
  .action(function (keys) {
    console.log(keys)
  })

program
  .command('list')
  .description('show apikeys list')
  .action(function () {

  })

program
  .command('edit')
  .description('edit apikeys in editor')
  .action(function () {

  })

program
  .command('clear')
  .description('remove all apikeys')
  .action(function () {

  })

program.parse(process.argv)
