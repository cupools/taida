#!/usr/bin/env node

var program = require('commander')
var PKG = require('../package.json')
var apikey = require('../lib/cmd/apikey')

program
  .version(PKG.version)
  .usage('<command> [keys]')

program
  .command('add <keys...>')
  .description('add apikeys for usage')
  .action(function (keys) {
    apikey.add(keys)
  })

program
  .command('delete <keys>')
  .description('remove apikeys by')
  .action(function (keys) {
    apikey.delete(keys)
  })

program
  .command('use <key>')
  .description('remove apikeys')
  .action(function (key) {
    console.log(key)
  })

program
  .command('list')
  .description('show apikeys list and compress count')
  .action(function () {

  })

program
  .command('edit')
  .description('edit apikeys in editor')
  .action(function () {

  })

program
  .command('clear')
  .description('remove all apikeys')
  .action(function () {

  })

program.parse(process.argv)
